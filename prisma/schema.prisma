// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id Int @id @default(autoincrement())
  name String
  email String
  password String
  username String
}

model containt {
    id String @id
    title String
    description String
    link String
    priority String
    timeToFinish Int
    status String
    createdAt DateTime
}